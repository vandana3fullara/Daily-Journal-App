<%- include("partials/header"); -%>
<h1>Compose Post</h1>
<form class="" action="/compose" method="post">
    <div class="form-group">
        <label>Post Title</label>
        <input class="form-control" type="text", name="postTitle">
        <label>Post Body</label>
        <textarea class="form-control" name="postBody" cols="30" rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-info" name="button">Publish</button> 
</form> 
<%- include("partials/footer"); -%>